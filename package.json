{
  "name": "discount-ascii-warehouse",
  "private": true,
  "version": "0.2.0",
  "description": "For all your ascii needs.",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "dev-server": "webpack-dev-server",
    "dev": "concurrently --kill-others \"yarn start\" \"yarn dev-server\"",
    "test": "REACT_APP_API_HOST='http://localhost:8080' jest --silent",
    "test:snapshots:regenerate": "yarn test -- --updateSnapshot",
    "test:debug": "REACT_APP_API_HOST='http://localhost:8080' jest",
    "test:coverage": "yarn test -- --coverage",
    "test:watch": "yarn test -- --watch --notify",
    "build": "webpack"
  },
  "author": "",
  "license": "",
  "dependencies": {
    "babel-jest": "^21.2.0",
    "babel-preset-stage-0": "^6.22.0",
    "cool-ascii-faces": "^1.3.4",
    "ecstatic": "^1.4.0",
    "html-webpack-plugin": "^2.28.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^21.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "recompose": "^0.26.0",
    "redux": "^3.6.0",
    "redux-actions": "^2.2.1",
    "redux-logger": "^3.0.1",
    "redux-mock-store": "^1.2.2",
    "redux-promise-middleware": "^4.4.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "concurrently": "^3.4.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "nock": "^9.0.24",
    "react-addons-test-utils": "^15.5.1",
    "redux-mock-store": "^1.2.2",
    "url-parse": "^1.2.0",
    "webpack": "^2.3.3"
  },
  "jest": {
    "testRegex": "(/__tests__/(.*|(/.|/))).(test|spec).(js?|jsx?)$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/static/**",
      "!**/index.{js,jsx}",
      "!**/__tests__/**"
    ]
  }
}
